Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"Newton", "[", 
    RowBox[{"xn_", ",", "f_", ",", "fp_"}], "]"}], ":=", 
   RowBox[{"xn", "-", 
    RowBox[{
     RowBox[{"f", "[", "xn", "]"}], "/", 
     RowBox[{"fp", "[", "xn", "]"}]}]}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"new", "=", 
   RowBox[{"Input", "[", "\"\<Initial guess\>\"", "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"prev", "=", "new"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"new", "=", 
   RowBox[{"Newton", "[", 
    RowBox[{"prev", ",", 
     RowBox[{"Function", "[", 
      RowBox[{"x", ",", 
       RowBox[{
        RowBox[{"x", "^", "2"}], "-", "1"}]}], "]"}], ",", 
     RowBox[{"Function", "[", 
      RowBox[{"x", ",", 
       RowBox[{"2", "x"}]}], "]"}]}], "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{"While", "[", 
  RowBox[{
   RowBox[{
    RowBox[{"Abs", "[", 
     RowBox[{"new", "-", "prev"}], "]"}], ">", "0.0001"}], ",", 
   "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"prev", "=", "new"}], ";", "\[IndentingNewLine]", 
    RowBox[{"new", "=", 
     RowBox[{"Newton", "[", 
      RowBox[{"prev", ",", 
       RowBox[{"Function", "[", 
