          RowBox[{"x", "^", "2"}], "-", "1"}]}], "]"}], ",", 
       RowBox[{"Function", "[", 
        RowBox[{"x", ",", 
         RowBox[{"2", "x"}]}], "]"}]}], "]"}]}], ";"}]}], 
  "\[IndentingNewLine]", "]"}], "\[IndentingNewLine]", 
