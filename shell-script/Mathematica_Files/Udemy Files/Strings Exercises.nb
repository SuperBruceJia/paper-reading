Cell["\<\
Given a real number, use string manipulation to figure out the number of \
digits before and after the decimal place. \
\>", "Text",
 CellChangeTimes->{{3.711134466122078*^9, 3.7111345067685013`*^9}}],

Cell[CellGroupData[{

Cell["Solution", "Subsubsection",
 CellChangeTimes->{{3.7111345114599648`*^9, 3.711134512293049*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"NumReport", "[", "r_", "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"s", "=", 
       RowBox[{"ToString", "[", "r", "]"}]}], ",", "parts", ",", "pre", ",", 
      "post"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"parts", "=", 
      RowBox[{"StringSplit", "[", 
       RowBox[{"s", ",", "\"\<.\>\""}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"pre", "=", 
      RowBox[{"StringLength", "@", 
       RowBox[{"parts", "[", 
        RowBox[{"[", "1", "]"}], "]"}]}]}], ";", 
     RowBox[{"post", "=", 
      RowBox[{"StringLength", "@", 
       RowBox[{"parts", "[", 
        RowBox[{"[", 
         RowBox[{"-", "1"}], "]"}], "]"}]}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"Grid", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{
         RowBox[{"{", 
          RowBox[{
          "\"\<Before Decimal Point\>\"", ",", 
           "\"\<After Decimal Point\>\""}], "}"}], ",", 
         RowBox[{"{", 
          RowBox[{"pre", ",", "post"}], "}"}]}], "}"}], ",", 
       RowBox[{"Frame", "\[Rule]", "All"}]}], "]"}]}]}], 
   "\[IndentingNewLine]", "]"}]}]], "Input",
 CellChangeTimes->{{3.7111345732095423`*^9, 3.711134756895754*^9}, {
  3.711134791503662*^9, 3.711134791815147*^9}}],

